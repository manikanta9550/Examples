class Solution{

public Pair<Integer, Integer> getMinMax(int[] arr) {
        int length=arr.length;
        int min=arr[0];
        int max=arr[0];
        
        
        for( int i=0;i<length;i++){
            if(arr[i]<min){
                min=arr[i];
            }
            if(arr[i]>max){
                max=arr[i];
            }
        }
        return new Pair<>(min,max);
    }

}
